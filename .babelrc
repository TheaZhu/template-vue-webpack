{
  "presets": [
    [ "env", { "modules": false } ],
    "es2015",
    "stage-2"
  ],
  "plugins": [
    [ "transform-runtime" ],
    "transform-object-rest-spread",
    "transform-object-assign",
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": [ "env", "stage-2" ],
      "plugins": [ ]
    }
  }
}
